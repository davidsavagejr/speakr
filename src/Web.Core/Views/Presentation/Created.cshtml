@model Web.Models.PresentationCreated

<div class="container-fluid">
    <h1>All Set</h1>
    <p>Now that your presentation is created, you give a talk now or schedule it for later.</p>
    <div class="media">
        <div class="media-left">
            <a href="@Url.Action("Create", "Talk", new { presentationId = @Model.NewId})"><img src="/Images/bubble-24.png" width="64" height="64"/></a>
        </div>
        <div class="media-body">
            <h4 class="media-heading">Talk Now</h4>
            Give a talk now using the presentation you just created.
        </div>
    </div>
    <hr/>
    <div class="media">
        <div class="media-left">
            <img src="/Images/time-24.png" width="64" height="64" />
        </div>
        <div class="media-body">
            <h4 class="media-heading">Schedule For Later</h4>
            Comming Soon.
        </div>
    </div>

</div>